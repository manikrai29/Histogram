<p id="notice"><%= notice %></p>

<h1>Listing Coursees</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @coursees.each do |coursee| %>
      <tr>
        <td><%= coursee.course_id %></td>
        <td><%= coursee.description %></td>
        <td><%= link_to 'Show', coursee %></td>
        <td><%= link_to 'Edit', edit_coursee_path(coursee) %></td>
        <td><%= link_to 'Destroy', coursee, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

